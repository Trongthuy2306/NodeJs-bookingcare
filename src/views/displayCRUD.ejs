<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        table,
        th,
        td {
            border: 1px solid black;
            border-collapse: collapse;
        }
    </style>
    <title>Display CRUD</title>
</head>

<body>
    <table style="width: 100%;">
        <tr>
            <th>Email</th>
            <th>Firstname</th>
            <th>Lastname</th>
            <th>Address</th>
        </tr>
        <tr>
            <% for(var i=0; i < dataTable.length; i++) { %>
        <tr>
            <td>
                <%= dataTable[i].email %>
            </td>
            <td>
                <%= dataTable[i].firstName %>
            </td>
            <td>
                <%= dataTable[i].lastName %>
            </td>
            <td>
                <%= dataTable[i].address %>
            </td>
        </tr>
        <% } %>
            </tr>
    </table>
</body>

</html>